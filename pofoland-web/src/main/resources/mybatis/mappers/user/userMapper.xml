<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hst.pofoland.biz.user.dao.UserDAO">

    <!-- 포트폴리오 기본정보 등록 -->
    <insert id="create" useGeneratedKeys="true" keyProperty="userNo">
        INSERT INTO MB_USER (
            USER_EMAIL,
            USER_PWD,
            USER_NICK_NM,
            USER_STT_CD,
            USER_JOIN_SE_CD,
            REG_DTM,
            UPD_DTM
        ) VALUES (
            #{userEmail},
            #{userPwd},
            #{userNickNm},
            #{userSttCd},
            #{userJoinSeCd},
            NOW(),
            NOW()
        )
    </insert>
</mapper>